CREATE TABLE ships_ais (Longitude FLOAT,Latitude FLOAT,unused_z DECIMAL(1,0),SOG FLOAT,COG FLOAT,Heading SMALLINT,ROT SMALLINT,BaseDateTime TIMESTAMP(0),Status SMALLINT,VoyageID VARCHAR(32),MMSI VARCHAR(32),ReceiverType VARCHAR(32),ReceiverID VARCHAR(16),Destination VARCHAR(32),Cargo SMALLINT,Draught SMALLINT,ETA TIMESTAMP(0),StartTime TIMESTAMP(0),EndTime TIMESTAMP(0),unused_IMO VARCHAR(16),unused_CallSign VARCHAR(16),unused_Name VARCHAR(16),VesselType VARCHAR(50),VesselLength SMALLINT,VesselWidth SMALLINT,DimensionComponents VARCHAR(16));


ALTER TABLE ships_ais ADD COLUMN id SERIAL PRIMARY KEY;

CREATE PUBLICATION aispub FOR TABLE ONLY ships_ais;

GRANT ALL PRIVILEGES ON ships_ais TO testuser;
