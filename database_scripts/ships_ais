CREATE TABLE ships_ais (Longitude FLOAT,Latitude FLOAT,unused_z DECIMAL(1,0),SOG FLOAT,COG FLOAT,Heading SMALLINT,ROT SMALLINT,BaseDateTime TIMESTAMP(0) ENCODING FIXED(32),Status SMALLINT,VoyageID TEXT ENCODING DICT(32),MMSI TEXT ENCODING DICT(32),ReceiverType TEXT ENCODING DICT(8),ReceiverID TEXT ENCODING DICT(16),Destination TEXT ENCODING DICT(32),Cargo SMALLINT,Draught SMALLINT,ETA TIMESTAMP(0) ENCODING FIXED(32),StartTime TIMESTAMP(0) ENCODING FIXED(32),EndTime TIMESTAMP(0) ENCODING FIXED(32),unused_IMO TEXT ENCODING DICT(16),unused_CallSign TEXT ENCODING DICT(16),unused_Name TEXT ENCODING DICT(16),VesselType TEXT ENCODING DICT(8),VesselLength SMALLINT,VesselWidth SMALLINT,DimensionComponents TEXT ENCODING DICT(16)) WITH (FRAGMENT_SIZE = 100000000);
